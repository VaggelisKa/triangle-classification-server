{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": 1,
        "linebreak-style": 0,
        "object-curly-spacing": 0,
        "no-extra-semi": 1,
        "indent": 1,
        "no-var": 2,
        "comma-dangle": 0,
        "spaced-comment": 1,
        "require-jsdoc": 0,
        "camelcase": 0,
        "operator-linebreak": 0,
        "no-unused-vars": 1,
        "no-trailing-spaces": 1,
        "max-len": 0,
        "arrow-parens": 0,
        "react/jsx-filename-extension": 0,
        "jsx-a11y/label-has-associated-control": 0,
        "object-curly-newline": 0,
        "react/prop-types": 0,
        "react/jsx-props-no-spreading": 0,
        "jsx-a11y/anchor-is-valid": 0,
        "react/style-prop-object": 0,
        "arrow-body-style": 1,
        "import/no-unresolved": 0,
        "linbreak-style": [0, "error", "unix"],
        "no-underscore-dangle": 0,
        "no-else-return": 0,
        "no-restricted-properties": 0,
        "consistent-return": 0
    }
}
